# check if the file gets created
$ absent target/frege/com/earldouglas/helloworld/HelloWorld.java
$ absent target/scala-2.4.0/classes/com/earldouglas/helloworld/HelloWorld.class
> compile
$ exists target/frege/com/earldouglas/helloworld/HelloWorld.java
$ exists target/scala-2.4.0/classes/com/earldouglas/helloworld/HelloWorld.class
